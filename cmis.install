<?php
/**
 * @file
 * Install, update and uninstall functions for the CMIS module.
 */

/**
 * Implementation of hook_uninstall().
 */
function cmis_uninstall() {
  // Delete variables.
  db_delete('variable')
    ->condition('name', db_like('cmis_vendor') . '%', 'LIKE')
    ->execute();
  cache_clear_all('variables', 'cache_bootstrap');
}
